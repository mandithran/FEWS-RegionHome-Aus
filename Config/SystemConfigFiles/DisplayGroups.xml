	<plot id="REPSPCTemplate">
		<subplot>
			<line>
				<color>blue</color>
				<lineStyle>solid</lineStyle>
				<axis>right</axis>
				<timeSeriesSet>
					<moduleInstanceId>PreprocessREPS</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>PC.nwp.acc</parameterId>
					<locationSetId>$MODELNAME1$Basins</locationSetId>
					<timeSeriesType>external forecasting</timeSeriesType>
					<timeStep unit="hour" multiplier="3"/>
					<readWriteMode>read only</readWriteMode>
					<ensembleId>REPS</ensembleId>
				</timeSeriesSet>
			</line>
		</subplot>
		<subplot>
			<line>
				<color>blue</color>
				<lineStyle>solid;thick</lineStyle>
				<axis>left</axis>
				<label>mean</label>
				<timeSeriesSet>
					<moduleInstanceId>PreprocessREPS</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>PC.nwp.acc</parameterId>
					<qualifierId>mean</qualifierId>
					<locationSetId>$MODELNAME1$Basins</locationSetId>
					<timeSeriesType>external forecasting</timeSeriesType>
					<timeStep unit="hour" multiplier="3"/>
					<readWriteMode>read complete forecast</readWriteMode>
				</timeSeriesSet>
			</line>
			<area>
				<color>blue</color>
				<opaquenessPercentage>50</opaquenessPercentage>
				<label>25%-75%</label>
				<timeSeriesSet>
					<moduleInstanceId>PreprocessREPS</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>PC.nwp.acc</parameterId>
					<qualifierId>25%</qualifierId>
					<locationSetId>$MODELNAME1$Basins</locationSetId>
					<timeSeriesType>external forecasting</timeSeriesType>
					<timeStep unit="hour" multiplier="3"/>
