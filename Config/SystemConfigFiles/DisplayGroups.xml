<displayGroups xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/displayGroups.xsd" version="1.0">
	<!--Plot Templates Section-->
	<!--Plot Template - WSC Data Data-->
	<plot id="AusAstroTideTemplate">
		<visibilityGroup>WaterLevel</visibilityGroup>
		<subplot>
			<line>
				<visibilityGroup>WaterLevel</visibilityGroup>
				<color>blue</color>
				<lineStyle>constant</lineStyle>
				<axis>left</axis>
				<visibleInLegend>true</visibleInLegend>
				<visibleInTable>true</visibleInTable>
				<visibleInPlot>true</visibleInPlot>
				<timeSeriesSet>
					<moduleInstanceId>ImportAstroTide</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>ATid.pred</parameterId>
					<locationSetId>AstroTideLocSet</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep unit="minute" multiplier="15"/>
					<readWriteMode>read complete forecast</readWriteMode>
				</timeSeriesSet>
			</line>
		</subplot>
	</plot>
	<displayGroup id="ImportAstroTidesDisp" name="Import Astronomical Tides">
		<nodeId>ImportAstroTidesNode</nodeId>
		<singleLocationDisplays>
			<locationSetId>AstroTideLocSet</locationSetId>
			<plotId>AusAstroTideTemplate</plotId>
		</singleLocationDisplays>
	</displayGroup>
</displayGroups>
