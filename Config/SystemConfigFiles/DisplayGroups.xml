	<!--Plot Templates Section-->
	<!--Plot Template - WSC Data Data-->
	<plot id="WSCHourlyTemplate">
		<visibilityGroup>Discharge</visibilityGroup>
		<visibilityGroup>WaterLevel</visibilityGroup>
		<subplot>
			<line>
				<visibilityGroup>Discharge</visibilityGroup>
				<color>blue</color>
				<lineStyle>constant</lineStyle>
				<axis>left</axis>
				<visibleInLegend>true</visibleInLegend>
				<visibleInTable>true</visibleInTable>
				<visibleInPlot>true</visibleInPlot>
				<timeSeriesSet>
					<moduleInstanceId>ImportWSC</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>QR.obs</parameterId>
					<locationSetId>$MODELNAME1$HydroStations</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep unit="minute" multiplier="5"/>
					<readWriteMode>read complete forecast</readWriteMode>
				</timeSeriesSet>
			</line>
			<line>
				<visibilityGroup>WaterLevel</visibilityGroup>
				<color>red</color>
				<lineStyle>dashdot</lineStyle>
				<axis>right</axis>
				<visibleInLegend>true</visibleInLegend>
				<visibleInTable>false</visibleInTable>
				<visibleInPlot>true</visibleInPlot>
				<timeSeriesSet>
					<moduleInstanceId>ImportWSC</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>HG.obs</parameterId>
					<locationSetId>$MODELNAME1$HydroStations</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep unit="minute" multiplier="5"/>
					<readWriteMode>read complete forecast</readWriteMode>
				</timeSeriesSet>
			</line>
		</subplot>
	</plot>
		<plot id="AusAstroTideTemplate">
		<visibilityGroup>WaterLevel</visibilityGroup>
		<subplot>
			<line>
				<visibilityGroup>WaterLevel</visibilityGroup>
				<color>blue</color>
				<lineStyle>constant</lineStyle>
				<axis>left</axis>
				<visibleInLegend>true</visibleInLegend>
				<visibleInTable>true</visibleInTable>
				<visibleInPlot>true</visibleInPlot>
				<timeSeriesSet>
					<moduleInstanceId>ImportAstroTide_hc</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>ATid.pred</parameterId>
					<locationSetId>AstroTideLocSet</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep unit="minute" multiplier="15"/>
					<readWriteMode>read complete forecast</readWriteMode>
				</timeSeriesSet>
			</line>
		</subplot>
	</plot>
	<!--Plot Template - RDPA Data-->
	<plot id="RDPATemplate">
		<subplot>
			<line>
				<color>red</color>
				<lineStyle>bar</lineStyle>
				<axis>left</axis>
				<label>RDPA Basin Precip -  hourly [mm]</label>
				<timeSeriesSet>
					<moduleInstanceId>PreprocessRDPA</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>PC.sim</parameterId>
					<locationSetId>$MODELNAME1$Basins</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep unit="hour" multiplier="6"/>
					<relativeViewPeriod unit="day" start="-14" end="0"/>
					<readWriteMode>add originals</readWriteMode>
				</timeSeriesSet>
			</line>
		</subplot>
	</plot>
