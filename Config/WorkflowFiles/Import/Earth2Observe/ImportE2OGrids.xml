<?xml version="1.0" encoding="UTF-8"?>
<workflow xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/workflow.xsd" version="1.1">
	<!-- Retrieve NWP grids from Environment Canada -->
	<properties>
		<!-- Input Grid-->
		<string key="INPUTGRID" value="E2O_25"/>
		<!-- Input Module Instance ID-->
		<string key="INPUTMODULEINSTANCE" value="ImportE2O"/>
		<!--Starttime-->
		<string key="STARTTIME" value="-365"/>
		<!--Endtime-->
		<string key="ENDTIME" value="0"/>
		<!--Timeseries Type-->
		<string key="TIMESERIESTYPE" value="external historical"/>
		<!--Timeseries Type-->
		<string key="TIMESTEP" value="24"/>
		<!--Ensemble ID-->
		<string key="ENSEMBLE" value="main"/>
	</properties>		
<!-- Import Earth2Observe Data -->
	<activity>
		<runIndependent>false</runIndependent>
		<moduleInstanceId>ImportE2O</moduleInstanceId>
	</activity>
<!-- Merge Earth2Observe Precipitation -->
	<activity>
		<runIndependent>false</runIndependent>
		<moduleInstanceId>MergeE2OPrecip</moduleInstanceId>
	</activity>
	<!--Pre-process Data for Model Run-->
	<activity>
		<runIndependent>false</runIndependent>
		<workflowId>PreprocessE2O$MODELNAME1$</workflowId>
	</activity>
	<!--Pre-process Data for Model Run-->
	<activity>
		<runIndependent>false</runIndependent>
		<workflowId>PreprocessE2O$MODELNAME2$</workflowId>
	</activity>
</workflow>
