	<spatialCopyModifier id="RDPSSpatialCopy" name="RDPS Spatial Copy">
		<expiryTime unit="day" multiplier="2"/>
		<timeSeries>
			<moduleInstanceId>ImportRDPS</moduleInstanceId>
			<parameterId>PC.nwp</parameterId>
			<locationId>RDPS</locationId>
		</timeSeries>
