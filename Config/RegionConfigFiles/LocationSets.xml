<locationSets version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/locationSets.xsd">
	<!-- All Location Sets Built from csv and shp files. See MapLayerFiles for source-->
	<!--Gridded products and Australia CSV file for tidal locationsets-->
	<locationSet id="Gridded_Products">
		<csvFile>
			<file>Gridded_Products</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%LOCID%</id>
			<name>%FULL_NAME%</name>
			<description>%DESCRIPTION%</description>
			<shortName>%LOCID%</shortName>
			<x>%LONG%</x>
			<y>%LAT%</y>
			<z>%Z%</z>
			<attribute id="STATE">
				<text>%STATE%</text>
			</attribute>
			<attribute id="REGION">
				<text>%REGION%</text>
			</attribute>
			<attribute id="Model">
				<text>%FULL_NAME%</text>
			</attribute>
			<attribute id="Radar">
				<text>%FULL_NAME%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<!-- Note that location sets can be combined or divided up using the Region attribute-->
	<locationSet id="AstroTideLocSet">
		<sortingLocationAttributeId>ID</sortingLocationAttributeId>
		<csvFile>
			<file>astroTideLocations.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%ID%</id>
			<name>%Name%</name>
			<x>%Longitude%</x>
			<y>%Latitude%</y>
			<attribute id="ID" name="Station Code">
				<text>%ID%</text>
			</attribute>
			<attribute id="Region" name="Region">
				<text>%Region%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<!-- These surge locations are based on the BOM NSS output.
	The Lat_NSS and Lon_NSS fields in the .csv file must
	be pre-determined by examining this output. -->
	<locationSet id="ausLocations">
		<sortingLocationAttributeId>ID</sortingLocationAttributeId>
        <csvFile>
            <file>surgeLocations.csv</file>
            <geoDatum>GDA94</geoDatum>
            <id>%ID%</id>
			<name>%Name%</name>
			<x>%Lon_NSS%</x>
			<y>%Lat_NSS%</y>
            <relation id="region">
                  <relatedLocationId>%REGION%</relatedLocationId>
            </relation>
        </csvFile>
    </locationSet>
</locationSets>
