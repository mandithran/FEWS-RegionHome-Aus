	<!--Gridded products and Australia CSV file for tidal locationsets-->
	<locationSet id="Gridded_Products">
		<csvFile>
			<file>Gridded_Products</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%LOCID%</id>
			<name>%FULL_NAME%</name>
			<description>%DESCRIPTION%</description>
			<shortName>%LOCID%</shortName>
			<x>%LONG%</x>
			<y>%LAT%</y>
			<z>%Z%</z>
			<attribute id="STATE">
				<text>%STATE%</text>
			</attribute>
			<attribute id="REGION">
				<text>%REGION%</text>
			</attribute>
			<attribute id="Model">
				<text>%FULL_NAME%</text>
			</attribute>
			<attribute id="Radar">
				<text>%FULL_NAME%</text>
			</attribute>
		</csvFile>
	</locationSet>
