	<locationSet id="WSCStations">
		<sortingLocationAttributeId>ID</sortingLocationAttributeId>
		<csvFile>
			<file>hydrometric_StationList.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%ID%</id>
			<name>%Name / Nom%</name>
			<x>%Longitude%</x>
			<y>%Latitude%</y>
			<attribute id="ID" name="Station Code">
				<text>%ID%</text>
			</attribute>
			<attribute id="Region" name="Region">
				<text>%Prov/Terr%</text>
			</attribute>
			<!-- Add attribute files for each model-->
			<attributeFile>
				<csvFile>hydrometric_$MODELNAME1$.csv</csvFile>
				<id>%ID%</id>
				<attribute id="$MODELNAME1$">
					<text>%$MODELNAME1$%</text>
				</attribute>
				<attribute id="LatCorrection">
				<number>%LatCorrection%</number>
				</attribute>
					<attribute id="LonCorrection">
					<number>%LonCorrection%</number>
				</attribute>
			</attributeFile>
			<attributeFile>
				<csvFile>hydrometric_$MODELNAME2$.csv</csvFile>
				<id>%ID%</id>
				<attribute id="$MODELNAME2$">
					<text>%$MODELNAME2$%</text>
				</attribute>
			</attributeFile>
		</csvFile>
	</locationSet>
	<!-- Note that location sets can be combined or divided up using the Region attribute-->
	<locationSet id="AstroTideLocSet">
		<sortingLocationAttributeId>ID</sortingLocationAttributeId>
		<csvFile>
			<file>astroTideLocations.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%ID%</id>
			<name>%Name%</name>
			<x>%Longitude%</x>
			<y>%Latitude%</y>
			<attribute id="ID" name="Station Code">
				<text>%ID%</text>
			</attribute>
			<attribute id="Region" name="Region">
				<text>%Region%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<!-- These surge locations are based on the BOM NSS output.
	The Lat_NSS and Lon_NSS fields in the .csv file must
	be pre-determined by examining this output. -->
	<locationSet id="Aus_Surge_Locations">
		<sortingLocationAttributeId>ID</sortingLocationAttributeId>
        <csvFile>
            <file>surgeLocations.csv</file>
            <geoDatum>GDA94</geoDatum>
            <id>%ID%</id>
			<name>%Name%</name>
			<x>%Lon_NSS%</x>
			<y>%Lat_NSS%</y>
            <relation id="region">
                  <relatedLocationId>%REGION%</relatedLocationId>
            </relation>
        </csvFile>
    </locationSet>
	<!--- ECCC Meteo Stations - Available at https://dd.weather.gc.ca/hydrometric/doc/ -->
	<locationSet id="ECCCStations">
		<csvFile>
			<file>swob-xml_station_list.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%MSC_ID%</id>
			<name>%Name%</name>
			<x>%Longitude%</x>
			<y>%Latitude%</y>
			<z>%Elevation(m)%</z>
			<attribute id="ID" name="Station Code">
				<text>%MSC_ID%</text>
			</attribute>
			<attribute id="Region" name="Region">
				<text>%Province/Territory%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<!--- ECCC Meteo Stations from Partners - Available at https://dd.weather.gc.ca/hydrometric/doc/ -->
	<locationSet id="ECCCPartnerStations">
		<csvFile>
			<file>swob-xml_partner_station_list.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%MSC%</id>
			<name>%EN name%</name>
			<x>%Longitude%</x>
			<y>%Latitude%</y>
			<z>%Elevation%</z>
			<attribute id="ID" name="Station Code">
				<text>%MSC%</text>
			</attribute>
			<attribute id="Region" name="Region">
				<text>%Province%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<!--Additional csv files-->
	<locationSet id="$MODELNAME2$MeteoStations">
		<csvFile>
			<file>meterological_$MODELNAME2$.csv</file>
			<geoDatum>WGS 1984</geoDatum>
			<id>%Name%</id>
			<name>%Name%</name>
			<x>%Longitude%</x>
			<y>%Latitude%</y>
			<z>%Elevation(m)%</z>
			<!--This attribute added for IdMapping-->
			<attribute id="StationIdx">
				<text>%StationIdx%</text>
			</attribute>
		</csvFile>
	</locationSet>
	<!--LocationSets built from ShapeFiles-->
	<!--ShapeFiles for $MODELNAME1$-->
	<locationSet id="$MODELNAME1$SubBasins">
		<esriShapeFile>
			<file>$MODELNAME1$Watersheds.shp</file>
			<id>%BasinName%</id>
			<name>%BasinName%</name>
		</esriShapeFile>
	</locationSet>
	<locationSet id="$MODELNAME1$Extent">
		<esriShapeFile>
			<file>$MODELNAME1$Extent.shp</file>
			<id>$MODELNAME1$Extent</id>
			<name>$MODELNAME1$Extent</name>
		</esriShapeFile>
	</locationSet>
	<!--ShapeFiles for $MODELNAME2$-->
	<locationSet id="$MODELNAME2$SubBasins">
		<esriShapeFile>
			<file>$MODELNAME2$Watersheds.shp</file>
			<id>%B%</id>
			<name>%B%</name>
			<attribute id="RavenBasin">
				<text>%B%</text>
			</attribute>
			<attribute id="SubBasin">
				<text>%Sub_B%</text>
			</attribute>
		</esriShapeFile>
	</locationSet>
	<locationSet id="$MODELNAME2$Extent">
		<esriShapeFile>
			<file>$MODELNAME2$Extent.shp</file>
			<id>$MODELNAME2$Extent</id>
			<name>$MODELNAME2$Extent</name>
			<attribute id="RavenBasin">
				<text>$MODELNAME2$Extent</text>
			</attribute>
		</esriShapeFile>
	</locationSet>
	<!-- Create LocationSets based on Attributes-->
	<locationSet id="WSCStationsByRegion">
		<subLocationSetIdFunction>WSCStations@Region@</subLocationSetIdFunction>
		<locationSetId>WSCStations</locationSetId>
	</locationSet>
	<!-- Build LocationSets for MODELNAME1-->
	<locationSet id="$MODELNAME1$HydroStations">
		<locationSetId>WSCStations</locationSetId>
		<constraints>
			<attributeTextEquals id="$MODELNAME1$" equals="Y"/>
		</constraints>
	</locationSet>
	<locationSet id="$MODELNAME1$Basins">
		<locationSetId>$MODELNAME1$SubBasins</locationSetId>
		<locationSetId>$MODELNAME1$Extent</locationSetId>
	</locationSet>

	<!-- Build LocationSets for MODELNAME2-->
	<locationSet id="$MODELNAME2$HydroStations">
		<locationSetId>WSCStations</locationSetId>
		<constraints>
			<attributeTextEquals id="$MODELNAME2$" equals="Y"/>
		</constraints>
	</locationSet>
	<locationSet id="$MODELNAME2$Basins">
		<locationSetId>$MODELNAME2$SubBasins</locationSetId>
		<locationSetId>$MODELNAME2$Extent</locationSetId>
	</locationSet>
	<!--Build LocationSets for Display-->
		<locationSet id="ModelBasins">
		<locationSetId>$MODELNAME1$Basins</locationSetId>
		<locationSetId>$MODELNAME2$Basins</locationSetId>
	</locationSet>
	<locationSet id="ModelMeteoStations">
		<locationSetId>ECCCStationsNT</locationSetId>
		<locationSetId>$MODELNAME2$MeteoStations</locationSetId>
	</locationSet>
	<locationSet id="ModelHydroStations">
		<locationSetId>$MODELNAME1$HydroStations</locationSetId>
		<locationSetId>$MODELNAME2$HydroStations</locationSetId>
	</locationSet>	
		<locationSet id="DisplayMeteoStations">
		<locationSetId>ECCCStationsNT</locationSetId>
		<locationSetId>$MODELNAME2$MeteoStations</locationSetId>
	</locationSet>
	<locationSet id="DisplayHydroStations">
		<!--<locationSetId>WSCStationsNT</locationSetId>-->
		<locationSetId>$MODELNAME1$HydroStations</locationSetId>
		<locationSetId>$MODELNAME2$HydroStations</locationSetId>
	</locationSet>	
	<locationSet id="DischargeDisplayLocations$MODELNAME1$">
		<locationSetId>$MODELNAME1$HydroStations</locationSetId>
	</locationSet>
	<locationSet id="DischargeDisplayLocations$MODELNAME2$">
		<locationSetId>$MODELNAME2$HydroStations</locationSetId>
	</locationSet>
	<!--Build additional LocationSets-->
	<locationSet id="ECCCAllStations">
		<locationSetId>ECCCPartnerStations</locationSetId>
		<locationSetId>ECCCStations</locationSetId>
	</locationSet>
