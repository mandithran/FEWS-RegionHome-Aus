<?xml version="1.0" encoding="UTF-8"?>
<thresholdGroups xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/thresholds.xsd">
	<eventExpiryTime unit="day" multiplier="2"/>
	<maxActionEventDuration unit="hour" multiplier="24"/>
	<thresholdGroup id="Flow Thresholds" name="Flow Thresholds">
		<defaultThreshold shortName="Flow Thresholds"/>
		<levelThreshold id="Alert Level" name="Alert Level" shortName="Alert Level">
			<upWarningLevelId>1</upWarningLevelId>
		</levelThreshold>
		<levelThreshold id="Major Flood Level" name="Major Flood Level" shortName="Major Flood Level">
			<upWarningLevelId>2</upWarningLevelId>
		</levelThreshold>
	</thresholdGroup>
</thresholdGroups>
