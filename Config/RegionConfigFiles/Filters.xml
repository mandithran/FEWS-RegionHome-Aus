<filters version="1.1" xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/filters.xsd">
	<!-- Add default filter Id -->
	<defaultFilterId>AstronomicalTides</defaultFilterId>
	<!-- Timeseries Sets -->
	<timeSeriesSets id="AstronomicalTides">
		<timeSeriesSet>
			<moduleInstanceId>ImportAstroTide</moduleInstanceId>
			<valueType>scalar</valueType>
			<parameterId>ATid.pred</parameterId>
			<locationSetId>AstroTideLocSet</locationSetId>
			<timeSeriesType>external historical</timeSeriesType>
			<timeStep unit="minute" multiplier="15"/>
			<relativeViewPeriod unit="day" start="-5" end="0"/>
			<readWriteMode>add originals</readWriteMode>
			<synchLevel>5</synchLevel>
		</timeSeriesSet>
	</timeSeriesSets>
	<filter id="AstroPreditions" name="Astronomical Tide Predictions">
		<mapExtentId>$REGION1$</mapExtentId>
		<child foreignKey="AstronomicalTides"/>
	</filter>
		<!-- Second Level Filters -->
	<filter id="AstronomicalTides" name="Astronomical Tides">
		<validationIconsVisible>false</validationIconsVisible>
		<timeSeriesSetsId>AstronomicalTides</timeSeriesSetsId>
	</filter>
</filters>
