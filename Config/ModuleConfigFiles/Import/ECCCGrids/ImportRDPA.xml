<timeSeriesImportRun xmlns="http://www.wldelft.nl/fews" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                xsi:schemaLocation="http://www.wldelft.nl/fews http://fews.wldelft.nl/schemas/version1.0/timeSeriesImportRun.xsd">
   <import>
      <general>
         <importType>CanadaMeteoWCS</importType>
         <serverUrl>https://geo.weather.gc.ca/geomet?SERVICE=WCS&amp;VERSION=2.0.1&amp;REQUEST=GetCoverage</serverUrl>
         <user>user</user>
         <password>password</password>
         <relativeViewPeriod unit="day" start="-7" end="0" startOverrulable="true" endOverrulable="true"/>
         <idMapId>IdImportCanadaWCS</idMapId>
      </general>
     <properties>
        <bool key="useSubset" value="false"/>
      </properties>
      <timeSeriesSet>
         <moduleInstanceId>ImportRDPA</moduleInstanceId>
         <valueType>grid</valueType>
         <parameterId>PC.sim</parameterId>
         <locationId>RDPA</locationId>
         <timeSeriesType>external historical</timeSeriesType>
         <timeStep unit="hour" multiplier="6"/>
         <readWriteMode>add originals</readWriteMode>
      </timeSeriesSet>
      <externUnit parameterId="PC.sim" unit="kg m-2"/>
   </import>
</timeSeriesImportRun>
