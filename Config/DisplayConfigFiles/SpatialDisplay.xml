	<gridPlotGroup id="ECCC_Scalar_Download" name="ECCC Meteo Stations">
		<gridPlot id="PrecipitationECCC" name="Precipitation Daily @ Stations [mm]">
			<dataLayer>
				<onlyCircles>true</onlyCircles>
				<circleSizeFunction>12</circleSizeFunction>
				<circleBorderSize>1</circleBorderSize>
				<circleBorderColor>gray</circleBorderColor>
				<timeSeriesSet>
					<moduleInstanceId>ImportECCCScalar</moduleInstanceId>
					<valueType>scalar</valueType>
					<parameterId>PC.obs</parameterId>
					<locationSetId>DisplayMeteoStations</locationSetId>
					<timeSeriesType>external historical</timeSeriesType>
					<timeStep id="$DAY_TIMESTEP$"/>
					<relativeViewPeriod unit="day" start="-15" end="0"/>
